{
    "$schema": "http://json.schemastore.org/prettierrc",
    "printWidth": 120,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "none",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "svelteSortOrder": "options-scripts-styles-markup",
    "bracketSameLine": true,
    "svelteBracketNewLine": false
}
